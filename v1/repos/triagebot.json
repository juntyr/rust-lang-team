{
  "org": "rust-lang",
  "name": "triagebot",
  "description": "Automation/tooling for Rust spaces",
  "homepage": "https://triage.rust-lang.org",
  "bots": [
    "rustbot"
  ],
  "teams": [
    {
      "name": "triagebot",
      "permission": "maintain"
    },
    {
      "name": "infra",
      "permission": "maintain"
    }
  ],
  "members": [],
  "branch_protections": [
    {
      "pattern": "master",
      "dismiss_stale_review": false,
      "mode": {
        "pr_required": {
          "ci_checks": [],
          "required_approvals": 1
        }
      },
      "allowed_merge_teams": []
    }
  ],
  "archived": false,
  "private": false,
  "auto_merge_enabled": true
}