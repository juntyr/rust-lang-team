{
  "org": "rust-lang",
  "name": "sync-team",
  "description": "Synchronize the team repository with the services we use",
  "homepage": null,
  "bots": [],
  "teams": [
    {
      "name": "infra",
      "permission": "write"
    }
  ],
  "members": [],
  "branch_protections": [
    {
      "pattern": "master",
      "dismiss_stale_review": false,
      "mode": {
        "pr_required": {
          "ci_checks": [
            "CI"
          ],
          "required_approvals": 1
        }
      },
      "allowed_merge_teams": [
        "infra-admins"
      ]
    }
  ],
  "archived": false,
  "private": false,
  "auto_merge_enabled": true
}