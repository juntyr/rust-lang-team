{
  "org": "rust-lang",
  "name": "git2-rs",
  "description": "libgit2 bindings for Rust",
  "homepage": "https://docs.rs/git2",
  "bots": [],
  "teams": [
    {
      "name": "cargo",
      "permission": "write"
    }
  ],
  "members": [],
  "branch_protections": [
    {
      "pattern": "master",
      "dismiss_stale_review": false,
      "mode": {
        "pr_required": {
          "ci_checks": [
            "Success gate"
          ],
          "required_approvals": 0
        }
      },
      "allowed_merge_teams": []
    }
  ],
  "archived": false,
  "private": false,
  "auto_merge_enabled": true
}